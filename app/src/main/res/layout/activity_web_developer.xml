<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".intentWebDeveloper.ActivityWebDeveloper">

    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:isScrollContainer="true"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="-44dp">


        <LinearLayout
            android:layout_width="406dp"
            android:layout_height="719dp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="3dp"
            tools:layout_editor_absoluteY="2dp">

            <LinearLayout
                android:layout_width="410dp"
                android:layout_height="241dp"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="200dp"
                app:layout_constraintTop_toTopOf="@+id/textView2"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="1dp">


                <Button
                    android:id="@+id/buttonAdroid"
                    android:layout_width="120dp"
                    android:layout_height="96dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="30dp"
                    android:background="@drawable/webpemulalogo" />

                <ImageView
                    android:layout_width="162dp"
                    android:layout_height="188dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginLeft="-140dp"
                    android:layout_marginTop="2dp"
                    android:background="@drawable/logoandroid"
                    android:drawableTint="@color/colorPrimary"
                    android:scaleX="1.5"
                    android:scaleY="1.5"
                    tools:ignore="MissingConstraints" />

                <TextView
                    android:layout_width="125dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="-140dp"
                    android:layout_marginTop="115dp"
                    android:text=" Web Pemula" />


                <Button
                    android:id="@+id/buttonWeb"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginLeft="90dp"
                    android:layout_marginTop="30dp"
                    android:background="@drawable/webdasarlogo" />

                <ImageView
                    android:layout_width="162dp"
                    android:layout_height="188dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginLeft="-130dp"
                    android:layout_marginTop="2dp"
                    android:background="@drawable/logoandroid"
                    android:scaleX="1.5"
                    android:scaleY="1.5"
                    tools:ignore="MissingConstraints" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="-130dp"
                    android:layout_marginTop="110dp"
                    android:layout_marginRight="40dp"
                    android:text="Web Dasar" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="410dp"
                android:layout_height="219dp"
                android:layout_marginLeft="-410dp"
                android:layout_marginTop="500dp"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="1dp"
                tools:layout_editor_absoluteY="346dp">

                <Button
                    android:id="@+id/buttonCyberSecurity"
                    android:layout_width="125dp"
                    android:layout_height="100dp"
                    android:layout_marginLeft="40dp"
                    android:layout_marginTop="40dp"
                    android:background="@drawable/webfundamentallogo" />

                <ImageView
                    android:layout_width="162dp"
                    android:layout_height="188dp"
                    android:layout_marginLeft="-145dp"
                    android:layout_marginTop="2dp"
                    android:background="@drawable/logoandroid"
                    tools:ignore="MissingConstraints" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="-140dp"
                    android:layout_marginTop="110dp"
                    android:text=" Web Fundamental" />


                <Button
                    android:id="@+id/buttonLearnDatabase"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_marginLeft="80dp"
                    android:layout_marginTop="40dp"
                    android:background="@drawable/webmendalamlogo" />

                <ImageView
                    android:layout_width="162dp"
                    android:layout_height="188dp"
                    android:layout_marginLeft="-130dp"
                    android:layout_marginTop="2dp"
                    android:background="@drawable/logoandroid"
                    tools:ignore="MissingConstraints" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="-130dp"
                    android:layout_marginTop="110dp"
                    android:text="Web Mendalam" />


            </LinearLayout>


        </LinearLayout>

    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>